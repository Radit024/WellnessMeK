                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTwelve"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{sleepScheduleVM.sleepScheduleModel.txtTwelve}"
                            style="@style/txtIntermedium14_1" tools:text="@string/lbl_12"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearDate3"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_weight="0.2"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStylegray_50cornerRadius">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtThus"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_18pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{sleepScheduleVM.sleepScheduleModel.txtThus}"
                            style="@style/txtInterregular12" tools:text="@string/lbl_thus"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtThirteen"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{sleepScheduleVM.sleepScheduleModel.txtThirteen}"
                            style="@style/txtIntermedium14_1" tools:text="@string/lbl_13"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearDate4"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_weight="0.2"
                        android:layout_width="0dp"
                        android:orientation="vertical" style="@style/groupStylecornerRadius_7">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFri"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_22pxh"
                            android:layout_marginStart="@dimen/_22pxh"
                            android:layout_marginTop="@dimen/_18pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{sleepScheduleVM.sleepScheduleModel.txtFri}"
                            style="@style/txtInterregular12_1" tools:text="@string/lbl_fri"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFourteen"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_16pxv"
                            android:layout_marginEnd="@dimen/_22pxh"
                            android:layout_marginStart="@dimen/_22pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:text="@{sleepScheduleVM.sleepScheduleModel.txtFourteen}"
