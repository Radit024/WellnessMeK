                            android:gravity="center"
                            android:id="@+id/btnLearnMore"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:paddingBottom="@dimen/_10pxv"
                            android:paddingEnd="@dimen/_19pxh"
                            android:paddingStart="@dimen/_19pxh"
                            android:paddingTop="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_learn_more"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded" tools:text="@string/lbl_learn_more"/>
                    </LinearLayout>
                </LinearLayout>
                <ImageView android:id="@+id/imageLayerOne"
                    android:layout_height="@dimen/_100pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_33pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="@dimen/_118pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_layer1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_layer1"/>
